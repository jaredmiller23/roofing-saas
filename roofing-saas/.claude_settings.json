{
  "sandbox": {
    "enabled": true,
    "autoAllowBashIfSandboxed": true
  },
  "permissions": {
    "defaultMode": "acceptEdits",
    "allow": [
      "Read(./**)",
      "Glob(./**)",
      "Grep(./**)",
      "Bash(*)",
      "Write(components/contacts/contact-form.tsx)",
      "Edit(components/contacts/contact-form.tsx)"
    ]
  }
}